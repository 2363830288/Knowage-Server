<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd"
	version="2.2">

	<persistence-unit name="knowage-gallery" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>

		<class>it.eng.knowage.knowageapi.dao.dto.SbiWidgetGallery</class>
		<class>it.eng.knowage.knowageapi.dao.dto.SbiWidgetGalleryTag</class>

		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mariadb://localhost:3310/knowage_master" />
			<property name="javax.persistence.jdbc.driver" value="org.mariadb.jdbc.Driver" />
			<property name="javax.persistence.jdbc.user" value="root" />
			<property name="javax.persistence.jdbc.password" value="root" />
		</properties>
	</persistence-unit>

	<persistence-unit name="knowage-functioncatalog" transaction-type="RESOURCE_LOCAL">
		<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>

		<class>it.eng.knowage.knowageapi.dao.dto.SbiCatalogFunction</class>
		<class>it.eng.knowage.knowageapi.dao.dto.SbiFunctionInputVariableId</class>
		<class>it.eng.knowage.knowageapi.dao.dto.SbiFunctionInputVariable</class>
		<class>it.eng.knowage.knowageapi.dao.dto.SbiObjFunction</class>
		<class>it.eng.knowage.knowageapi.dao.dto.SbiFunctionInputColumn</class>
		<class>it.eng.knowage.knowageapi.dao.dto.SbiFunctionInputColumn$Pk</class>
		<class>it.eng.knowage.knowageapi.dao.dto.SbiFunctionOutputColumn</class>
		<class>it.eng.knowage.knowageapi.dao.dto.SbiFunctionOutputColumn$Pk</class>

		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mariadb://localhost:3310/knowage_master" />
			<property name="javax.persistence.jdbc.driver" value="org.mariadb.jdbc.Driver" />
			<property name="javax.persistence.jdbc.user" value="root" />
			<property name="javax.persistence.jdbc.password" value="root" />
		</properties>
	</persistence-unit>

</persistence>
